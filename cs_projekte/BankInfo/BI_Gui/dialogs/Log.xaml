<Window x:Class="BI_Gui.dialogs.LogViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Log" Height="440" Width="788" WindowStyle="SingleBorderWindow" Closing="Window_Closing">
    <Grid>
        <ListView Name="listLog" Width="auto" Margin="0,0,0,29" />
        <Button Name="btnClose" Content="Schließen" Click="Button_Click" HorizontalAlignment="Right" Width="75" Height="23" VerticalAlignment="Bottom" />
        <Button Name="btnClear" Content="Ausleeren" HorizontalAlignment="Right" Margin="0,0,81,0" Width="75" Height="23" VerticalAlignment="Bottom" Click="Button_Click" />
    </Grid>
</Window>
